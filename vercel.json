{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sort",
      "dest": "/api/sort.js"
    },
    {
      "src": "/api/data",
      "dest": "/api/data.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/data.js"
    }
  ]
}
